{
  "extends": [
    "react-app"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-uses-react": "error",
    "react/react-in-jsx-scope": "error",
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
