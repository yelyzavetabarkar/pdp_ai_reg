+import { useState, useEffect } from 'react';
+
+export function useApprovals(managerId: any) {
+  const [data, setData] = useState<any>(null);
+  const [error, setError] = useState<any>(null);
+
+  useEffect(() => {
+    fetch(`/api/managers/${managerId}/approvals`)
+      .then(r => r.json())
+      .then(setData)
+      .catch(setError);
+  }, []);
+
+  return { data, error, loading: !data && !error };
+}
+
+export function useManagerStats(managerId: any) {
+  const [stats, setStats] = useState<any>({});
+
+  // TODO: Implement this (2019)
+  return { stats };
+}
