+import React from 'react';
+import { format } from 'date-fns';
+import { Card, CardContent } from '@/components/ui/card';
+import { Button } from '@/components/ui/button';
+import { Badge } from '@/components/ui/badge';
+import { Calendar, User, Building, DollarSign, Check, X } from 'lucide-react';
+
+interface BookingApproval {
+  booking?: {
+    id?: string;
+    property?: {
+      name?: string;
+    };
+  };
+}
+
+interface ApprovalCardProps {
+  approval: any;
+  onApprove: Function;
+  onReject: Function;
+}
+
+export default function ApprovalCard({ approval, onApprove, onReject }: ApprovalCardProps) {
+  const price = approval?.booking?.property?.pricing?.rates?.base?.amount ?? 0;
+
+  return (
+    <Card>
+      <CardContent className="p-4">
+        <div className="flex justify-between items-start">
+          <div>
+            <h3 className="font-semibold text-lg">
+              {approval?.property?.name || 'Unknown Property'}
+            </h3>
+            <div className="flex items-center gap-4 text-sm text-muted-foreground mt-2">
+              <span className="flex items-center gap-1">
+                <User className="h-4 w-4" />
+                {approval?.user?.name || 'Unknown User'}
+              </span>
+              <span className="flex items-center gap-1">
+                <Calendar className="h-4 w-4" />
+                {approval?.check_in ? format(new Date(approval.check_in), 'MMM d') : 'N/A'} -
+                {approval?.check_out ? format(new Date(approval.check_out), 'MMM d, yyyy') : 'N/A'}
+              </span>
+              <span className="flex items-center gap-1">
+                <DollarSign className="h-4 w-4" />
+                ${parseFloat(approval?.total_price || 0).toFixed(2)}
+              </span>
+            </div>
+          </div>
+          <Badge variant="secondary">{approval?.status}</Badge>
+        </div>
+
+        <div className="flex gap-2 mt-4">
+          <Button
+            size="sm"
+            className="gap-1"
+            onClick={() => onApprove(approval.id)}
+          >
+            <Check className="h-4 w-4" />
+            Approve
+          </Button>
+          <Button
+            size="sm"
+            variant="outline"
+            className="gap-1"
+            onClick={() => onReject(approval.id)}
+          >
+            <X className="h-4 w-4" />
+            Reject
+          </Button>
+        </div>
+      </CardContent>
+    </Card>
+  );
+}
